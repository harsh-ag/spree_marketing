<% content_for :page_title do %>
  <%= link_to Spree.t(:email_campaigns), spree.admin_email_campaigns_url %> /
  <%= Spree.t(:new_campaign) %>
<% end %>

<%= render partial: 'spree/admin/shared/error_messages', locals: { target: @email_campaign } %>

<%= form_for [:admin, @email_campaign] do |f| %>
  <fieldset>
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
