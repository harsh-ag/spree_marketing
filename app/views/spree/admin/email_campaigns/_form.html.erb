<div data-hook="admin_contact_form_fields">
  <%= f.field_container :template_name, class: ['form-group'], 'data-hook' => 'template_name' do %>
    <%= f.label :template_name, Spree.t(:template_name) %>
    <%= f.text_field :template_name, class: 'form-control' %>
    <%= f.error_message_on :template_name %>
  <% end %>

  <%= f.field_container :email_list, class: ['form-group'] do %>
    <%= f.label :spree_email_list_id, Spree.t(:email_list) %>
    <%= f.select :spree_email_list_id, Spree::EmailList.pluck(:name, :id), {}, class: 'select2' %>
  <% end %>

</div>
